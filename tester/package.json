{
    "name": "tester",
    "author": "aurospire",
    "version": "0.0.0",
    "license": "MIT",
    "main": "dist/index.ts",
    "types": "dist/index.d.ts",
    "files": [
        "/dist"
    ],
    "exports": {
        ".": "./dist/index.js"
    },
    "typesVersions": {
        "*": {}
    },
    "config": {
        "watch": "./src;package*.json"
    },
    "scripts": {
        "start": "node ./dist/index.js",
        "watch": "nodemon -e ts,js,tsx,jsx,json --watch src --watch 'package*.json' --exec 'tsc -p . && node ./dist/index.js || exit 1'",
        "build": "tsc -p ."
    },
    "dependencies": {
        "tablebook": "file:..",
        "varcor": "^0.8.0"
    },
    "devDependencies": {
        "@types/node": "^22.12.0",
        "nodemon": "^3.1.9",
        "ts-node": "^10.9.2",
        "typescript": "^5.7.3"
    }
}
